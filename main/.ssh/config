Host *github.com
    ForwardX11 no
    ForwardX11Trusted no
Host *
    ForwardX11 yes
    ForwardX11Trusted yes
    ServerAliveCountMax 60
    ServerAliveInterval 10
    StrictHostKeyChecking no
    ControlMaster auto
    ControlPath ~/.ssh/sockets/%r@%h-%p
    ControlPersist 600
