[user]
  email = william_povell@brown.edu
  name = Will Povell
[push]
  default = current
[alias]
  s  = status
  a  = add
  aa = add .
  ca = commit -am
  c  = commit -m
  ch = checkout
  b  = branch
  st = stash
  sp = stash pop
  r  = reset
  d  = diff
  p  = push -u
  pf = push -f
  pl = pull
  l  = log
  uncommit = reset HEAD^
  open = !emacs  `~/bin/support/git-open`
