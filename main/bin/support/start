#!/bin/bash

PATH=$PATH:~/bin/support

if [[ "$1" = "-r" ]]; then
  retheme    
  exit
fi

retheme startup # Blocks

start-emacs &
compton 2>/dev/null &
unclutter &
kitty --class=dropdown /bin/bash 
dunst >/tmp/dunst.log 2>&1 &
redshift &
polybar &
startdropbox
startlocker &
spotify &

i3-msg workspace "1: I"
