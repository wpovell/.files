s = '''
           {0}&&            
          &&&&&
        &&&{1}\/{0}& &&&
       &&{1}|,/  |/{0}& &&
        &&{1}/   /  /_{0}&  &&{1}
          \  {{  |_____/_{0}&{1}
          {{  / /          {0}&&&{1}
          `, \{{___________/_{0}&&{1}
           }} }}{{       \\
           }}{{{{         \\____{0}&{1}
          {{}}{{           `{0}&{1}\{0}&&{1}
          {{{{}}             {0}&&{1}
    , -=-~{{ .-^- _
          `}}
           {{

'''.format(flower, tree)

